time = mTime.c # header file to measure time in Linux Ubuntu
teslaK80_arch = -gencode arch=compute_37,code=sm_37 # Optimization compilation code request for the Tesla K80 GPU.
geForceGtx1660Super_arch = -gencode arch=compute_75,code=sm_75 # Optimization compilation code request for the GeForce GTX 1660 SUPER GPU.

main.x:
