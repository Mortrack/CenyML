# Create the following directory variables.
oLib = ../../../../"CenyML_library_skeleton"/otherLibraries
cM = ../../../../"CenyML_library_skeleton"/CenyML_Library
time = mTime.c # header file to measure time in Linux Ubuntu

main.x:
	gcc main_raisetoTheFirstPowerRegression.c $(oLib)/time/$(time) $(oLib)/csv/csvManager.c $(oLib)/pbPlots/pbPlots.c $(oLib)/pbPlots/supportLib.c $(cM)/cpuSequential/evaluationMetrics/CenyMLregressionEvalMet.c $(cM)/cpuParallel/machineLearning/CenyMLdeepLearning_PC.c -O3 -o main_raisetoTheFirstPowerRegression.x -lpthread -lm
	gcc main_simpleLinearRegressionWithRelu.c $(oLib)/time/$(time) $(oLib)/csv/csvManager.c $(oLib)/pbPlots/pbPlots.c $(oLib)/pbPlots/supportLib.c $(cM)/cpuSequential/evaluationMetrics/CenyMLregressionEvalMet.c $(cM)/cpuParallel/machineLearning/CenyMLdeepLearning_PC.c -O3 -o main_simpleLinearRegressionWithRelu.x -lpthread -lm
	gcc main_hyperbolicTangentRegression.c $(oLib)/time/$(time) $(oLib)/csv/csvManager.c $(oLib)/pbPlots/pbPlots.c $(oLib)/pbPlots/supportLib.c $(cM)/cpuSequential/evaluationMetrics/CenyMLregressionEvalMet.c $(cM)/cpuParallel/machineLearning/CenyMLdeepLearning_PC.c -O3 -o main_hyperbolicTangentRegression.x -lpthread -lm
	gcc main_logisticRegression.c $(oLib)/time/$(time) $(oLib)/csv/csvManager.c $(oLib)/pbPlots/pbPlots.c $(oLib)/pbPlots/supportLib.c $(cM)/cpuSequential/evaluationMetrics/CenyMLregressionEvalMet.c $(cM)/cpuParallel/machineLearning/CenyMLdeepLearning_PC.c -O3 -o main_logisticRegression.x -lpthread -lm
	gcc main_raiseToTheSecondPowerRegression.c $(oLib)/time/$(time) $(oLib)/csv/csvManager.c $(oLib)/pbPlots/pbPlots.c $(oLib)/pbPlots/supportLib.c $(cM)/cpuSequential/evaluationMetrics/CenyMLregressionEvalMet.c $(cM)/cpuParallel/machineLearning/CenyMLdeepLearning_PC.c -O3 -o main_raiseToTheSecondPowerRegression.x -lpthread -lm
	gcc main_raiseToTheThirdPowerRegression.c $(oLib)/time/$(time) $(oLib)/csv/csvManager.c $(oLib)/pbPlots/pbPlots.c $(oLib)/pbPlots/supportLib.c $(cM)/cpuSequential/evaluationMetrics/CenyMLregressionEvalMet.c $(cM)/cpuParallel/machineLearning/CenyMLdeepLearning_PC.c -O3 -o main_raiseToTheThirdPowerRegression.x -lpthread -lm
	gcc main_raiseToTheFourthPowerRegression.c $(oLib)/time/$(time) $(oLib)/csv/csvManager.c $(oLib)/pbPlots/pbPlots.c $(oLib)/pbPlots/supportLib.c $(cM)/cpuSequential/evaluationMetrics/CenyMLregressionEvalMet.c $(cM)/cpuParallel/machineLearning/CenyMLdeepLearning_PC.c -O3 -o main_raiseToTheFourthPowerRegression.x -lpthread -lm
	gcc main_raiseToTheFifthPowerRegression.c $(oLib)/time/$(time) $(oLib)/csv/csvManager.c $(oLib)/pbPlots/pbPlots.c $(oLib)/pbPlots/supportLib.c $(cM)/cpuSequential/evaluationMetrics/CenyMLregressionEvalMet.c $(cM)/cpuParallel/machineLearning/CenyMLdeepLearning_PC.c -O3 -o main_raiseToTheFifthPowerRegression.x -lpthread -lm
	gcc main_raiseToTheSixthPowerRegression.c $(oLib)/time/$(time) $(oLib)/csv/csvManager.c $(oLib)/pbPlots/pbPlots.c $(oLib)/pbPlots/supportLib.c $(cM)/cpuSequential/evaluationMetrics/CenyMLregressionEvalMet.c $(cM)/cpuParallel/machineLearning/CenyMLdeepLearning_PC.c -O3 -o main_raiseToTheSixthPowerRegression.x -lpthread -lm
	gcc main_firstOrderExponentialRegression.c $(oLib)/time/$(time) $(oLib)/csv/csvManager.c $(oLib)/pbPlots/pbPlots.c $(oLib)/pbPlots/supportLib.c $(cM)/cpuSequential/evaluationMetrics/CenyMLregressionEvalMet.c $(cM)/cpuParallel/machineLearning/CenyMLdeepLearning_PC.c -O3 -o main_firstOrderExponentialRegression.x -lpthread -lm
	gcc main_secondOrderExponentialRegression.c $(oLib)/time/$(time) $(oLib)/csv/csvManager.c $(oLib)/pbPlots/pbPlots.c $(oLib)/pbPlots/supportLib.c $(cM)/cpuSequential/evaluationMetrics/CenyMLregressionEvalMet.c $(cM)/cpuParallel/machineLearning/CenyMLdeepLearning_PC.c -O3 -o main_secondOrderExponentialRegression.x -lpthread -lm
	gcc main_linearClassification.c $(oLib)/time/$(time) $(oLib)/csv/csvManager.c $(oLib)/pbPlots/pbPlots.c $(oLib)/pbPlots/supportLib.c $(cM)/cpuSequential/evaluationMetrics/CenyMLclassificationEvalMet.c $(cM)/cpuParallel/machineLearning/CenyMLdeepLearning_PC.c -O3 -o main_linearClassification.x -lpthread -lm
	gcc main_raisetoTheFirstPowerRegression2.c $(oLib)/time/$(time) $(oLib)/csv/csvManager.c $(oLib)/pbPlots/pbPlots.c $(oLib)/pbPlots/supportLib.c $(cM)/cpuSequential/evaluationMetrics/CenyMLregressionEvalMet.c $(cM)/cpuParallel/machineLearning/CenyMLdeepLearning_PC.c -O3 -o main_raisetoTheFirstPowerRegression2.x -lpthread -lm

